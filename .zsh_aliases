alias python="python3"
alias tnew="tmux new -s"
alias tls="tmux ls"
alias tsend="tmux send -t"
alias tkill="tmux kill-session -t"
alias tat="tmux attach-session -t"
alias tata="tmux attach-session -t 0"
alias tmuxm='cd && tmux new-session \; split-window -v \; split-window -h \; split-window -v \; select-pane -L \; attach \; resize-pane -x 120 \;'
alias vimrc="vim ~/.vimrc"
alias zshrc="vim ~/.zshrc"
alias reload="source ~/.zshrc"
alias gad="git add"
alias gst="git status"
alias gbr="git branch"
alias gcm="git commit -m"
alias gdf="git diff"
alias ggp="git grep"
alias glog="git log --oneline --color | emojify | less -r"
alias greset="git reset HEAD -"
alias diff="diff --color=auto"
alias fdir='find . -type d -name'
alias ffil='find . -type f -name'
alias grep="grep --color=auto"
alias apt="sudo dnf"
alias apt-get="sudo dnf"
alias dnf="sudo dnf"
alias q="exit"
alias pu='pip-review --auto'
alias nu='sudo npm update -g'
alias du='sudo dnf update -y'
alias sysd="sudo systemctl disable"
alias syse="sudo systemctl enable"
alias sysr="sudo systemctl restart"
alias syss="systemctl status"
alias systa="sudo systemctl start"
alias systo="sudo systemctl stop"
alias x="xplr"
alias xcd='cd "$(xplr)"'
alias sudo = 'sudo -E'
alias drmv="docker volume ls | grep -v DRIVER | awk {'print $2'} | xargs docker volume rm"
alias dpsa="docker ps -a"
alias dps="docker ps"
alias dcd="docker-compose down"
alias dcu="docker-compose up -d"
